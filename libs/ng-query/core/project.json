{
  "name": "ng-query-core",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/ng-query/core/src",
  "prefix": "lib",
  "projectType": "library",
  "release": {
    "version": {
      "manifestRootsToUpdate": ["dist/{projectRoot}"],
      "currentVersionResolver": "git-tag",
      "fallbackCurrentVersionResolver": "disk"
    }
  },
  "tags": [],
  "targets": {
    "buildLib": {
      "executor": "@nx/angular:package",
      "outputs": ["{workspaceRoot}/dist/{projectRoot}"],
      "options": {
        "project": "libs/ng-query/core/ng-package.json",
        "tsConfig": "libs/ng-query/core/tsconfig.lib.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/ng-query/core/tsconfig.lib.prod.json"
        },
        "development": {}
      },
      "defaultConfiguration": "production"
    },
    "buildSchematics": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc -p libs/ng-query/core/tsconfig.schematics.json && copyfiles -u 3 libs/ng-query/core/schematics/*/schema.json libs/ng-query/core/schematics/*/files/** libs/ng-query/core/schematics/collection.json dist/libs/ng-query/core/"
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          { "command": "nx buildLib ng-query-core" },
          { "command": "nx buildSchematics ng-query-core" }
        ],
        "parallel": false
      }
    },
    "nx-release-publish": {
      "options": {
        "packageRoot": "dist/{projectRoot}"
      }
    },
    "test": {
      "executor": "@nx/vite:test",
      "outputs": ["{options.reportsDirectory}"],
      "options": {
        "reportsDirectory": "../../../coverage/libs/ng-query/core"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    }
  }
}
